scalar Time

extend type Building @key(fields: "id") {
    id: ID! @external
    entrances: [Entrance!]!
}

type Entrance @key(fields: "id") {
    id: ID!
    name: String!
    telemetryData(startTime: Time!, endTime: Time): [TelemetryData!]!
}

type TelemetryData {
    timestamp: String!
    value: Int
}

type Query {
    getEntrances(ids: [ID!]): [Entrance!]!
}
