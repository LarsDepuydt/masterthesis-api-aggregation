scalar Time

extend type Building @key(fields: "id") {
    id: ID! @external
    entrances: [Entrence!]!
}

type Entrence {
    id: ID!
    name: String!
    telemetryData(startTime: Time!, endTime: Time): [TelemetryData!]!
}

type TelemetryData {
    timestamp: String!
    value: Int
}

type Query {
    getEntrences(ids: [ID!]): [Entrence!]!
}
