scalar Time

type DepartmentParticipation {
    department: String!
    attendee_count: Int!
    is_external: Boolean!
}

type Event {
    eventID: String!
    subject: String!
    start: String!
    end: String!
    durationMinutes: Int!
    department_breakdown: [DepartmentParticipation!]!
    form_participants: String
    form_department: String
}

extend type Room @key(fields: "id") {
    id: ID! @external
    name: String!
    email: String!
    events(startTime: Time!, endTime: Time): [Event!]!
}

type Query {
    rooms(emails: [String!]): [Room!]!
}
